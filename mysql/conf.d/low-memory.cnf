[mysqld]
# Reduce memory usage to avoid OOM kills during init
innodb_buffer_pool_size=32M
innodb_log_file_size=32M
max_connections=40
key_buffer_size=8M
thread_stack=192K
thread_cache_size=4
table_open_cache=200
table_definition_cache=400
query_cache_size=0
performance_schema=OFF
skip-name-resolve
skip-host-cache
